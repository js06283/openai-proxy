{
	"version": 2,
	"builds": [
		{
			"src": "api/openai-proxy.js",
			"use": "@vercel/node"
		},
		{
			"src": "api/threads.js",
			"use": "@vercel/node"
		},
		{
			"src": "thread-viewer.html",
			"use": "@vercel/static"
		}
	],
	"routes": [
		{
			"src": "/",
			"dest": "thread-viewer.html"
		},
		{
			"src": "/openai-proxy",
			"dest": "api/openai-proxy.js"
		},
		{
			"src": "/api/threads",
			"dest": "api/threads.js"
		},
		{
			"src": "/thread-viewer",
			"dest": "thread-viewer.html"
		}
	]
}
